. "$(x wsroot)"/lib/env
test_env_one(){
    @assert stdout "data | ___x_cmd_jo_env aaa=.files\\\\.defaultLanguage" << A
aaa="shellscript";
A
    @assert stdout " data | ___x_cmd_jo_env . .files\\\\.associations.index\\\\.html" << A
html="shellscript";
A
    @assert stdout "data | ___x_cmd_jo_env . .files\\\\.associations.\\\*\\\\.json" << A
json="json";
A
}
test_env_multi(){
    @assert stdout " data | ___x_cmd_jo_env . aaa=.files\\\\.defaultLanguage bbb=.files\\\\.defaultLanguage" << A
aaa="shellscript";bbb="shellscript";
A
}

aa(){
    echo $vvv
}
bb(){
    echo $vvv
    echo $bbb
}
test_env_fun(){
    @assert stdout "data | ___x_cmd_jo_env . vvv=.files\\\\.defaultLanguage -- aa" << A
shellscript
A
    @assert stdout "data | ___x_cmd_jo_env . vvv=.files\\\\.defaultLanguage www=.files\\\\.trimTrailingWhitespace -- bb " << A
shellscript
true
A

}

@assert:file:func