
cp -r ./* ~/.x-cmd/jo
. ./lib/query

test_query_eqarr_all(){

    x jo "$(cat .vscode/settings.json )" | ___x_cmd_jo_query 1.files\\.associations
}

test_query_eqarr(){

    x jo "$(cat .vscode/settings.json )" | ___x_cmd_jo_query 1.associations 1.index\\.html
    x jo "$(cat .vscode/settings.json)"  | ___x_cmd_jo_query 1.explicitFolding\\.rules
    x jo "$(cat .vscode/settings.json )" | ___x_cmd_jo_query 1.explicitFolding\\.rules.awk 1.1.endRegex
}

test_query_eqarr_all

